<div class="fso_card" style="background:url('assets/img/cards/[@card_color]')">
    <div class="card_title">[@card_title]</div>
    <div class="card_subtype_symbol"><img class="card_tier" src="[@card_tier]"></img></div>
    <div class="card_imgcanvas [@card_subtype_color]" style="background-image:url('assets/img/cards/[@card_img]'); background-size:100% 100%;">
        <div class="card_type_text">
            <b>[@card_type]</b>
        </div>
        <div class="card_type"></div>

        <div class="card_type_symbol [@card_type_color]"></div>
    </div>
    <div class="card_subtype"><b>[@card_subtype]</b> - <font class="card_species">[@card_species]</font></div>
    <div class="card_description">
        <b>[@card_keyword]</b> - [@card_description]<br>
    </div>
    <div class="card_flavor">
        <i>[@card_flavor]</i>
    </div>
</div>